#customConsole
  position fixed
  bottom -2em
  left 0
  width 100%
  height 2em
  background-color black
  opacity 0.6
  transition bottom 0.2s ease-in, opacity 0.6s

  input
    margin-left 6%
    width 94%
    height 100%
    border none
    outline none
    background-color black
    color white
    font-size 1em
